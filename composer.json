{
    "name": "evolver/cross-domain-policy-module",
    "description": "Zend Framework 2 Module to provide cross-domain policy files",
    "require": {
        "php": ">=5.4",
        "zendframework/zend-http": "~2.3",
        "zendframework/zend-modulemanager": "~2.3",
        "zendframework/zend-mvc": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "zendframework/zend-console": "~2.3",
        "zendframework/zend-form": "~2.3",
        "zendframework/zend-log": "~2.3",
        "zendframework/zend-serializer": "~2.3",
        "zendframework/zend-view": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Evolver\\CrossDomainPolicyModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Evolver\\CrossDomainPolicyModuleTest\\": "tests/src/"
        }
    },
